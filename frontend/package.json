{
  "name": "docaccounting",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lk & npm run build:arm",
    "start:lk": "ng serve --env=dev --proxy-config lk.proxy.config.json --app=lk --port 4203",
    "build:lk": "ng build -app=lk --prod --aot=true --output-hashing=none  --deploy-url /wwwroot & npm run copy:lk",
    "start:arm": "ng serve --env=dev --proxy-config arm.proxy.config.json --app=arm --port 4203",
    "build:arm": "ng build -app=arm --prod --aot=true --output-hashing=none  --deploy-url /wwwroot/ & npm run copy:arm",
    "copy:lk": "xcopy \"./dist/apps/lk/*\" \"./../../../DocAccountingSystem/PersonalCabinet/wwwroot/\" /E /F /Y",
    "copy:arm": "xcopy \"./dist/apps/arm/*\" \"./../../../DocAccountingSystem/ARM/wwwroot/\" /E /F /Y",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format": "./node_modules/.bin/nx format write",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update check",
    "update:skip": "./node_modules/.bin/nx update skip",
    "postinstall": "./node_modules/.bin/nx postinstall"
  },
  "private": true,
